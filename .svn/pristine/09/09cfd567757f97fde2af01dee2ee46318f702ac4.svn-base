<#import "../../macro/macrocommon.htm" as common>
<@common.header/>
<@common.editheader name="我的通讯录"/>
<form id="formId" method="post" action="?action=update" >
<input type="hidden" name=id value="${obj.id?default('')}" /><input type="hidden" name=page value="${page?c}"  /><table cellSpacing=1 cellPadding=2 width="96%" align=center bgColor=#cccccc border=0 class="tab">
<thead>
<tr>
<td colspan=4 bgColor=#f8f8f8 height=30 align="center">
编辑我的通讯录</td>
</tr>
</thead>
<tbody>
<tr>
<td class="titleTd">姓名</td>
<td bgColor=#ffffff colspan="3"><input type="text" class="btinput" required=true name="tname" value="${obj.tname?default('')}" /><span class="spantip" id="tname_span"/></td>
</tr>
<tr>
<td class="titleTd">手机号码</td>
<td class="contentTd"><input type="text" class="txtinput" check="isMobile" name="mobilNumber" value="${obj.mobilNumber?default('')}" /><span class="spantip" id="mobilNumber_span"/></td>
<td class="titleTd">电子邮箱</td>
<td class="contentTd"><input type="text" class="txtinput" check="isEmail" name="email" value="${obj.email?default('')}" /><span class="spantip" id="email_span"/></td>
</tr>
<tr>
<td class="titleTd">QQ号码</td>
<td class="contentTd"><input type="text" class="txtinput" check="isInt;isString(5,12)" name="qqNumber" value="${obj.qqNumber?default('')}" /><span class="spantip" id="qqNumber_span"/></td>
<td class="titleTd">家庭地址</td>
<td class="contentTd"><input type="text" class="txtinput" name="homeAddress" value="${obj.homeAddress?default('')}" /><span class="spantip" id="homeAddress_span"/></td>
</tr>
<tr>
<td class="titleTd">工作地址</td>
<td class="contentTd"><input type="text" class="txtinput" name="workAddress" value="${obj.workAddress?default('')}" /><span class="spantip" id="workAddress_span"/></td>
<td class="titleTd">通讯录组</td>
<td class="contentTd">
<select  name="addressBookTypeId" /><@common.options list=addressBookTypelist defaultValue="${obj.addressBookTypeId?default('')}" display="false"/></select>
</td>
</tr>
<tr>
<td colspan=4 bgColor=#f8f8f8 height=30 align="center">
<input type="button" class="buttons" value="更新" onClick="save()" />&nbsp;&nbsp;&nbsp;&nbsp;<input type="button" class="buttons" value="关闭" onclick="javascript:history.go(-1);" />
</td>
</tr>
</tbody>
</table>
</form>
<@common.bottom/>

