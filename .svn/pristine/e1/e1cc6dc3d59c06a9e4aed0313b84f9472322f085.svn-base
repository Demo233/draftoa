<#import "../../macro/macrocommon.htm" as common>
<@common.header/>
<@common.editheader name="权限"/>
<form id="formId" method="post" action="?action=update" >
<input type="hidden" name=id value="${obj.id?default('')}" />
<input type="hidden" name=page value="${page?c}"  />
<table cellSpacing=1 cellPadding=2 width="96%" align=center bgColor=#cccccc border=0 class="tab">
<thead>
<tr>
<td colspan=4 bgColor=#f8f8f8 height=30 align="center">
编辑权限</td>
</tr>
</thead>
<tbody>
<tr>
<td class="titleTd">名称</td>
<td class="contentTd"><input type="text" class="txtinput" name="tname" value="${obj.tname?default('')}" /></td>
<td class="titleTd">菜单名称</td>
<td class="contentTd"><input type="text" class="txtinput" name="menuId" value="${obj.menuid?default('')}" /></td>
</tr>
<tr>
<td class="titleTd">菜单名称</td>
<td class="contentTd"><input type="text" class="txtinput" name="menuName" value="${obj.menuName?default('')}" /></td>
<td class="titleTd">角色名称</td>
<td class="contentTd"><input type="text" class="txtinput" name="roleId" value="${obj.roleid?default('')}" /></td>
</tr>
<tr>
<td class="titleTd">角色名称</td>
<td bgColor=#ffffff colspan="3"><input type="text" class="txtinput" name="roleName" value="${obj.roleName?default('')}" /></td>
</tr>
<tr>
<td class="titleTd">备注描述</td>
<td bgColor=#ffffff colspan="3"><textarea name="description" />${obj.description?default('')}" </textarea></td>
</tr>
<tr>
<td colspan=4 bgColor=#f8f8f8 height=30 align="center">
<input type="button" class="buttons" value="更新" onclick="save()" />&nbsp;&nbsp;&nbsp;&nbsp;<input type="button" class="buttons" value="关闭" onclick="javascript:history.go(-1);" />
</td>
</tr>
</tbody>
</table>
</form>
<@common.bottom/>

