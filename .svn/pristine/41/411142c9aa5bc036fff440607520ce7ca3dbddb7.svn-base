<!DOCTYPE html PUBLIC "-//WAPFORUM//DTD XHTML Mobile 1.0//EN" "http://www.wapforum.org/DTD/xhtml-mobile10.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Insert title here</title>
<link href="theme/defaultstyle/images/defaultstyle.css" rel="stylesheet" type="text/css" />
<script type="text/javascript" src="jqueryui/jquery-1.11.2.min.js"></script>
<script type="text/javascript" src="jqueryui/starsoft.js"></script>
</head>
<body>
<br>
<p align=center><font class="messageinfo">${msg}</font></p>
<div id="timediv" align=center>还剩<SPAN id="miao"></SPAN>秒将自动关闭</div>
<p align="center">
 <input id="but" type="button" onClick="colseMessagePage();" value="关闭" class="buttons" name="Preview">
</p>
<SCRIPT LANGUAGE = JavaScript>
var second=3;
var timer;
document.getElementById("miao").innerHTML=second;
function change(){
  second--;
 if(second>-1) {
  document.getElementById("miao").innerHTML=second;
  timer = setTimeout('change()',1000);//调用自身实现
 }else {
	clearTimeout(timer);
	timer=null;
	window.close();
 }
}
timer = setTimeout('change()',1000);
function colseMessagePage(){
	clearTimeout(timer);
	timer=null;
	window.close();
}
</SCRIPT>
</body>
</html>