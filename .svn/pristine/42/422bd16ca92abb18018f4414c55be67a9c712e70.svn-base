* { margin: 0; padding: 0; }
html { font-size:12px; margin:0px; padding:0px; }
body {margin:0 auto; padding:0; list-style:none;font-size:12px;text-align:center; font-family:Verdana, "宋体", sans-serif;background:#ffffff;color:#333333;line-height:150%;}
body>table,body>div { margin-left:auto; margin-right:auto; clear:both;}


/* css site */
/*--------------------------------------------------------*/
table,form,div,img,td,ul,li,dl,dt,dd {margin:0 auto; padding:0; border:0;}
div,td,li,dt,dd,h2,h3,h4,p,a { list-style-type:none; word-break:break-all; Width:fixed;}
select, input { vertical-align:middle; }
img {border:0; }
em{font-style:normal;}
/* 公用属性 */
/*--------------------------------------------------------*/
/* == clearfix == */
.clearfix:after { /*/*/ content: "."; /* IE8 hack */ display: block; height: 0; clear: both; visibility: hidden; }
/* IE7 hack */
.clearfix { display: inline-block; }
/* IE-mac, IE5, IE6 */
* html .clearfix { height: 1%; }
.clearfix { display: block; }

.fl{float:left;}
.fr{float:right;}

/* 连接属性 */
/*--------------------------------------------------------*/
a{text-decoration:none; color:#333333;}
a:hover{color:#eb6100;text-decoration:underline;}
#box{width:100px; height:130px;background-color:black;position:fixed;_position:absolute; top:400px; right:100px;z-index:10000}

/*-------------------------------------------------------- 页面布局开始 -------------------------------------------------------- */
/* wrapper */
/*--------------------------------------------------------*/
#page{margin:0 auto;text-align:center;background: #e2e2e2}
#page_sub{margin:0 auto;text-align:center;background: #e2e2e2 url("images/content_sub_bg.png") repeat-y center ;}
/* header_wrap */
/*--------------------------------------------------------*/
#header{height:150px;background:url("images/nav_bg.png") repeat-x  bottom;position:relative;z-index:52;}
.header{width:1000px;position:relative;}

#logo{width:529px;height:73px;background:url("images/logo.gif") no-repeat;text-indent:-9999px;position:absolute;top:15px; left:0;}
#header_search{height:30px;width:420px;position:absolute;top:38px;right:0;background:#ebebeb;}
#header_search .input{width:240px;height:28px;border-top:1px solid #cdcdcd;border-bottom:1px solid #cdcdcd;border-left:1px solid #cdcdcd;background:#fff;position:absolute;top:0;left:80px;line-height:28px;padding:0 0 0 5px;font-size:14px;}
#header_search #select{width:83px;height:30px;border:none;position:absolute;top:0;left:0;z-index:1000;}
#header_search .submit{width:93px;height:30px;background:#1374ca;color:#fff;font-size:18px;font-family:"微软雅黑";position:absolute;right:0;top:0;border:none;cursor: pointer;}
.selectbox-wrapper {	border: #cdcdcd 1px solid; margin-top: -2px; width: 81px; position: absolute; background-color: #fff; text-align: left;}
.selectbox-wrapper ul li.selected {background-color: #e0ecf7;}
.selectbox-wrapper ul li.current {color: #fff; background-color: #36c}
.selectbox-wrapper ul li {padding:6px; font-size:14px; width: 70px; cursor: pointer; line-height: 20px;}
.selectbox { border:none;  display: block; padding:8px 0 0 6px; width: 81px; height: 22px;cursor: pointer;   background:url(images/search_select.png) no-repeat;font-size:14px;}


 /* menu */
#main_nav{height:37px;position:absolute;left:0;top:100px;}
#menu ul{height:37px;margin:0 auto;margin:0 0;}
#menu li{float:left;text-decoration: none; font-family:"微软雅黑"}
#menu li a,#menu li a:hover{}
.activedTab,.nTab a,.nTab a:hover{display:block;width:90px; height:37px;line-height:37px;font-size:14px;font-weight:bold;text-align:center; }
.activedTab{background:#1374ca;}
.activedTab a{color:#fff;display:block;text-decoration:none;}
.activedTab a:hover{color:#eb6100;}
.nTab a{ color: #111111;}
.nTab a:hover{text-decoration:none;color:#fff;background:#1374ca;}

#submenu{padding:0 0 5px 0;margin:0 0 10px 0;background:#c4e5f5 url("images/sub_nav_bg.png") repeat;}
#submenu h3{margin:0;height:32px;border-bottom:2px solid #408ded;color:#408ded;font-size:30px;text-align:left;font-family:"微软雅黑";font-weight:normal;padding:20px 0 0 10px;}
#submenu ul{margin:10px auto;}
#submenu li{text-decoration: none; }
#sub-menu li a,#sub-menu li a:hover{}
.activedSubTab,.nTabSub a,.s-nTabSub a:hover{display:block;height:30px;line-height:30px;font-size:14px;text-align:right;padding:0 30px 0 0;background-repeat:no-repeat ;background-position:right;}
.activedSubTab{background-image:url("images/sub_menu_bg.png");}
.activedSubTab a{color:#333333;display:block;text-decoration:none;}
.activedSubTab a:hover{color:#333333;}
.nTabSub a{ color: #333333;}
.nTabSub a:hover{color: #333333;text-decoration:none;background-color:#e6f3f9;background-image:url("images/sub_menu_bg.png");}

/* buttons*/
.btn{background-image:url("images/btn.gif");background-color:transparent;background-repeat: no-repeat;}
.btn-qq{width:97px;height:21px;border:0;cursor: pointer;background-position:0 0;}
.btn-qq-hover,
.btn-qq:hover{background-position:0 0;}
.btn-qq-active,
.btn-qqlogin:active{background-position:0 0;}

.btn-leaveword{width:97px;height:21px;border:0;cursor: pointer;background-position:0 -100px;}
.btn-leaveword-hover,
.btn-leaveword:hover{background-position:0 -100px;}
.btn-leaveword-active,
.btn-leaveword:active{background-position:0 -100px;}

