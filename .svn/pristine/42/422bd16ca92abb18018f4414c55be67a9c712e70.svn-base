/*文章分页样式*/
#fenye{height:30px;margin:0 auto;text-align:center;color:#333; }
#fenye a{color:#333; }
 .fenye {clear:both;margin:0 auto; }
 .fenye a { margin:0 4px 0 0; color:#333; background:#fff; display:inline-table; border:1px solid #dcdddd;  text-align:center; height:20px; line-height:20px;text-decoration:none; }
 .fenye a.prev, .fenye a:hover .prev { width:48px; color:#333; text-align:center;text-decoration:none; line-height:21px; }
 .fenye a.next, .fenye a:hover .next { width:48px;color:#333; text-align:center;text-decoration:none; line-height:21px; }
 .fenye a.curr, .fenye a:hover .curr { width:20px; color:#333; font-weight:bold; text-align:center;text-decoration:none; }
 .fenye a.num, .fenye a:hover .num { width:20px;color:#333; text-decoration:none; }
 .fenye span{ margin:0 4px 0 0; color:#333; text-align:center; height:22px; line-height:22px;}
 .fenye a:hover { color:#fff; background:#c1e4f4; border:1px solid #408ded; }
 .fenye .zd{width:30px;height:22px;line-height:22px;border:1px #a7a7a7 solid;text-align:center;}
 .fenye .go{height:24px;width:24px;border:1px #a7a7a7 solid;background:#fff;}

