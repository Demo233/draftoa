body {
    color: #000000;
	background-color:#f1f1f1;
}

li > a[selected] > *:not(.toolbar), li > a:active > *:not(.SpeedDial){
    background-image: url(listArrowSel.png), url(selection.png) !important;
}

li > a[selected="progress"] >*:not(.SpeedDial) {
    background-image: url(../default/loading.gif), url(selection.png) !important;
}

/************************************************************************************************/

body > .toolbar {
    background:#c60000 url(toolbar.png) top  repeat-x;
}

.button {
    -webkit-border-image: url(toolButton.png) 0 5 0 5;
    -moz-border-image: url(toolButton.png) 0 5 0 5;
}

/*#backButton {
    -webkit-border-image: url(backButtonBrdr.png) 0 9 0 18;
    -moz-border-image: url(backButtonBrdr.png) 0 9 0 18;
    background: url(backButtonBack.png) repeat-x;
	 
}*/
#backButton {
    background: url(back.png) no-repeat;	 
}
#homeButton {
    background: url(home.png) no-repeat;	 
}
#addButton {
	top:1px;
	right:0px;
	width:43px;
	height:43px;
    background: url(btn-add.png) no-repeat; 
}
#saveButton {
	top:1px;
	right:0px;
	width:43px;
	height:43px;
    text-indent:-9999px;
    background: url(btn-save2.png) no-repeat; 
}
#confirmButton{background:url(btn-confirm.png) no-repeat;width:59px;height:33px;}
.whiteButton {
    -webkit-border-image: url(whiteButton.png) 0 12 0 12;
    -moz-border-image: url(whiteButton.png) 0 12 0 12;
    text-shadow: rgba(255, 255, 255, 0.7) 0 1px 0;
}
.redButton {
    -webkit-border-image: url(redButton.png) 0 12 0 12;
    -moz-border-image: url(redButton.png) 0 12 0 12;
}

.grayButton {
    -webkit-border-image: url(grayButton.png) 0 12 0 12;
    -moz-border-image: url(grayButton.png) 0 12 0 12;
    color: #FFFFFF;
}

/************************************************************************************************/
body > ul > li.group {
	opacity:0.7;
    background: url(listGroup.png) repeat-x;
}

body > ul > li > a {
    background: url(listArrow.png) no-repeat right center;
}
body > .panel {    
     background-color:#f1f1f1;
 }
body > .panel1 {    
    background-color:#f1f1f1;
}

.thumb {
    background: #ffffff url(thumb.png) repeat-x;
}

.panel > .inbox{
   /*** -webkit-border-image: url(inboxBg.png) 2 0 6 0;
    -moz-border-image: url(inboxBg.png) 2 0 6 0;***/
    text-shadow: rgba(255, 255, 255, 0.7) 0 1px 0; 
}
.form{ position: relative;
    min-height: 42px;	
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    text-align: right;
}
.loginSubmit{
    background:url("line-dashed.png") repeat-x top;
	min-height: 60px;
    position: relative;
    -webkit-border-radius: 0;
    -moz-border-radius: 0;
    text-align: right;
	padding:15px 0 0 0;
}
.loginSubmit > .loginBtn{
 -webkit-border-image: url(btn3.png) 0 10 0 10;
    -moz-border-image: url(btn3.png) 0 10 0 10;
    text-shadow: rgba(0, 0, 0, 0.7) 0 1px 0;
	border-width: 0 10px;
	width:65%;
	height:36px;
    text-align: center;
    font-size: 20px;
    font-weight: bold;
    color: #ffffff;

}
/************************************************************************************************/
#preloader {
    display: none;
    background-image: url(loading.gif), url(selection.png),
        url(blueButton.png), url(listArrowSel.png), url(listGroup.png);
}

.toolbar > h1.titleImg {
  background: url(title-img.png) no-repeat top center;
  color: rgba(0,0,0,0);
}

.backButtonImg {
  width: 50px;
  background: url(back-img.png) no-repeat center left,
              url(backButtonBack.png) repeat-x top left !important;
  color: rgba(0,0,0,0);
}

.select{background-image:url("selectArrow.png");background-repeat:no-repeat;background-position: right;}

.search{height:39px;background:#e4e4e4 url("search-bg.png") repeat-x;}
.search-input-bg{border:none;height:39px;width:auto;background: url("search-input-bg.png") no-repeat;}
.search-input{border:none;height:22px;width:230px;margin:0;position:absolute;left:8px;top:8px;}
.searchBtn{width:60px;height:30px;position:absolute;right:8px;top:5px;background-image: url("searchBtn.png") ;border:none;background-color:none;text-indent:-9999px;}