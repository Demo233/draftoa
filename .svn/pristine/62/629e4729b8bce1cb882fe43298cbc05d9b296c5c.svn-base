HTML {
	HEIGHT: 100%;
	margin: 0px; padding: 0px; color: rgb(85, 85, 85); font-family: 微软雅黑; font-size: 13px;
}
BODY {
	margin:0px;
	font-size: 13px;
	background-color: #FFFFFF;
	PADDING-RIGHT: 0px; PADDING-LEFT: 0px; 
	PADDING-BOTTOM: 0px; MARGIN: 0px; PADDING-TOP: 0px; 
	HEIGHT: 98%;
}
a {
	font-size: 13px;
	color: #000000;
	text-decoration: none;
}
.clearfloat {clear:both;height:0;font-size: 1px;line-height: 0px;}
/*******************************表格样式**************start**************************/
.tab {
	font-size: 13px;
	bgcolor:#CCCCCC;
}
thead tr{
	height:25px;font-size: 14px;
}
thead td{
	height:25px;text-align: center;background-color: #f8f8f8;font-size: 14px;
}
tbody tr{
	height:25px;font-size: 12px;background-color: #000000;
}

tbody td{
	height:24px;background-color: #ffffff;font-size: 13px;padding-left: 5px;
}
.titleTd{
	width:100px;height:25px;background-color: #F8F8F8;font-size: 13px;letter-spacing:1px;text-align: right;padding-right: 10px;
}
.titleTd2{
	width:50px;height:25px;background-color: #F8F8F8;font-size: 13px;letter-spacing:1px;text-align: right;padding-right: 10px;
}
.contentTd{
	background-color: #ffffff;font-size: 13px;letter-spacing:1px;width: 300px;text-align: left;
}
.contentTd2{
	background-color: #ffffff;font-size: 13px;letter-spacing:1px;text-align: left;
}
tfoot tr{
	height:25px;font-size: 12px;
}
tfoot td{
	height:22px;text-align: right;background-color: #f8f8f8;
}
/*******************************表格样式**************end**************************/
/*******************************列表导航功能样式**************start**************************/
.current {
	height:30px;
	margin-top:8px;
	margin-bottom: 2px;
}
.current .place {
	display:block;
	height:30px;
	line-height:30px;
	color: #666;
	float:left;
	font-size:16px;
	padding-left:20px;
}
.current .subplace {
	height:28px;
	line-height:28px;
	color: #666;
	float:left;
	font-size:13px;
}
.current  ul{
	float:right;
	margin:0px;
}
.current  ul li{
	float:right;
	margin-right:5px;
	list-style-type:none;
}
/*******************************列表导航功能样式**************end**************************/
.content {
	height:500px;
	border-left-width: 1px;
	border-left-style: double;
	border-left-color: #e1e1e1;
	background-color: #FFFFFF;
	background-image: url(index_66.jpg);
	background-repeat: repeat-x;
	background-position: top;
	padding-top:15px;
}
.buttons{ background:url(button.gif) no-repeat center; width:81px; height:28px; line-height:25px;text-align:center; color:#FFF; border:0px; cursor:pointer;}
.btn {
	PADDING-RIGHT: 15px; BORDER: #ccc 1px solid; PADDING-LEFT: 15px; FONT-SIZE: 14px;COLOR: black;cursor: pointer;
}
.reddiv {
	color: red;
}
.formsearch {
    background:#fff;
    border:1px solid #ccc;
    color:#000;
    font-family:verdana,tahoma,sans-serif;
    font-size:13px;
    width:196px;
    height:22px;
    margin-left:6px;
    padding-left:20px;
    line-height:20px;
    background:url(password.gif) no-repeat 2px 2px;
}
div#shadediv {
	Z-INDEX: 10; BACKGROUND: #aaa; FILTER: alpha(opacity=80); LEFT: 0px; WIDTH: 100%; POSITION: absolute; TOP: 0px; HEIGHT: 100%; opacity: 0.8
}
div#dialog {
	Z-INDEX: 20; WIDTH: 300px; POSITION: absolute; HEIGHT: 150px;
}
div#dialogtitlediv {
	BACKGROUND: #0853CC; WIDTH: 100%; CURSOR: move; HEIGHT: 30px; POSITION: absolute;font-size:13px; 
}
div#dialogtitle {
	WIDTH: 200px; HEIGHT: 22px;padding-top: 3px;padding-left: 10px;color: white; POSITION: absolute;font-weight: 600;font-size:large;
}
/****************************** form表单样式 ******************************/
.redfont{
	color: red;
	font-size: 13px;
}
.spantip{
	color: red;
	font-size: 12px;
	padding-left: 10px;
}
/** input文本框 **/
select {
	border: 1px solid rgb(191, 191, 191); height: 24px; line-height: 24px;
}
/** 必填表单 **/
.btinput {font-size:13px;border:#A5A5A5 1px solid;background-color: #FFFFDF;width:220px; height: 22px; line-height: 22px;padding-left: 5px;}
.btinputwidth {font-size:13px;border:#A5A5A5 1px solid;background-color: #FFFFDF;width:400px; height: 22px; line-height: 22px;padding-left: 5px;}
.bttextarea{font-size:13px;border:#A5A5A5 1px solid;background-color: #FFFFDF;}
.textarea{font-size:13px;border:#A5A5A5 1px solid;}
.txtinput {
	border: 1px solid rgb(191, 191, 191);font-size:13px; height: 22px; line-height: 22px;padding-left: 5px;
}
.txtinputwidth {
	border: 1px solid rgb(191, 191, 191);font-size:13px; height: 22px;width: 400px; line-height: 22px;padding-left: 5px;
}
.calendars {
	border: 1px solid rgb(191, 191, 191);font-size:13px; height: 22px; line-height: 22px;padding-left: 5px;
}
.width_220px {
	width: 220px;
}
.width_200px {
	width: 200px;
}




