<#import "../../macro/macrocommon.htm" as common>
<@common.header/>
<@common.addheader name="角色"/>
<form id="formId" method="post" action="?action=save" >
<table cellSpacing=1 cellPadding=2 width="96%" align=center bgColor=#cccccc border=0 class="tab">
<thead>
<tr>
<td colspan=4 bgColor=#f8f8f8 height=30 align="center">
新建角色</td>
</tr>
</thead>
<tbody>
<tr>
<td class="titleTd">角色名称</td>
<td  class="contentTd"><input type="text" class="btinput" required=true name="tname" style="width: 60%"/><span class="spantip" id="tname_span"/></td>
<td class="titleTd">角色标识</td>
<td class="contentTd"><input type="text" class="btinput" required=true name="id" check="strStartsWith('R')" value="${id?default('')}" style="width: 60%"/><span class="spantip" id="id_span"/></td>
</tr>
<tr>
<td colspan=4 bgColor=#f8f8f8 height=30 align="center">
<input type="button" class="buttons" value="保存" onclick="save()" />&nbsp;&nbsp;&nbsp;&nbsp;<input type="button" class="buttons" value="关闭" onclick="javascript:history.go(-1);" />
</td>
</tr>
</tbody>
</table>
</form>
<@common.bottom/>

