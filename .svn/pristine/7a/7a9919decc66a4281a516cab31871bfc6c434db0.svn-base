    <!-- 进度条 -->
    <script type="text/javascript">
    	function getNum(val){
    		if(val <100){
        		val = val +"%";
        		
        	}else{
        		val = 100 + "%";
        	}
    		return val;
    	}
    </script>
    <script type="text/javascript">
    	
    	var fyCount_div = document.getElementById("fyCountID");
    	var fy = getNum(fyCount_div.innerHTML);
    	fyCount_div.style.width = fy;
    	
    	var cbCount_div = document.getElementById("cbCountID");
    	var cb = getNum(cbCount_div.innerHTML);
    	cbCount_div.style.width = cb;
    	
    	var tgCount_div = document.getElementById("tgCountID");
    	var tg = getNum(tgCount_div.innerHTML);
    	tgCount_div.style.width = tg;
    	
    	var laCount_div = document.getElementById("laCountID");
    	var la = getNum(laCount_div.innerHTML);
    	laCount_div.style.width = la;
    </script>
    
   
    
    
