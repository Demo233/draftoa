<#import "../../macro/macrocommon.htm" as common>
<#import "../../macro/paginationCommon.htm" as pagination>
<@common.header/>
<div class="current">
 <span class="place">首页> >系统运行日志列表 </span>
</div>
<table  cellSpacing=1 cellPadding=2 width="100%" align=center bgColor=#cccccc border=0 class="tab">
<form id="commonform" action="?action=list" method="post">
<input type="hidden" id="sortfield" name="sortfield" value="${sortfield?default('')}" />
<input type="hidden" id="sortvalue" name="sortvalue" value="${sortvalue?default('')}" />
<input type='hidden' id='page' name='page' value='${page.pageIndex?c}' />
<tr>
<td bgColor=#f8f8f8 height=22 width=100>请选择筛选条件</td>
<td bgColor=#f8f8f8>日志标题：<input class="txtinput"  id="tname" name="tname" value="${tname?default('')}"></td>
<td bgColor=#f8f8f8 width=60><input type="image" id="ImageButton1" src="theme/defaultstyle/images/search.gif" onclick="searchSubmit()"	border="0" /></td>
</tr>
</form>
</table>
<table cellSpacing=1 cellPadding=2 width="100%" align=center bgColor=#cccccc border=0 class="tab">
<thead>
<tr>
<td width=20><input type="checkbox" name="allbox" value="checkbox" onClick="selectAll(this.checked)" /></td>
<td onclick="resetSort('tname')" style="cursor:pointer;" >日志标题<span id="sorttname"></span></td>
<td onclick="resetSort('qpath')" style="cursor:pointer;" >请求路径<span id="sortqpath"></span></td>
<td onclick="resetSort('infoClass')" style="cursor:pointer;" >请求类<span id="sortinfoClass"></span></td>
<td onclick="resetSort('createId')" style="cursor:pointer;" >请求帐号<span id="sortcreateId"></span></td>
<td onclick="resetSort('browserType')" style="cursor:pointer;" >浏览器<span id="sortbrowserType"></span></td>
<td onclick="resetSort('id')" style="cursor:pointer;" >请求时间<span id="sortid"></span></td>
</tr>
</thead>
<tbody>
<#list list as obj>
<tr ondblclick="readInfor('${obj.id}')">
<td width=20 align="center"><input type="checkbox" name="box" value="${obj.id?default('')}"/></td>
<td bgColor=#ffffff><a href='javascript:void(0)' onClick="readInfor('${obj.id}')" >${obj.tname?default('')}</a></td>
<td bgColor=#ffffff><a href='javascript:void(0)' onClick="readInfor('${obj.id}')" >${obj.qpath?default('')}</a></td>
<td bgColor=#ffffff>${obj.infoClass?default('')}</td>
<td bgColor=#ffffff><a href='javascript:void(0)' onClick="readInfor('${obj.id}')" >${obj.createId?default('')}</a></td>
<td bgColor=#ffffff>${obj.browserType?default('')}</td>
<td bgColor=#ffffff><@common.stringShowDate str="${obj.id}"/></td>
</tr>
</#list>
</tbody>
</table>
<table width="100%" border="0" cellpadding="0" cellspacing="0" class="tab">
<tfoot>
<tr>
<td height="22" align="right" bgcolor="#ebeaf4"><@pagination.pagination page=page />&nbsp;</td>
</tr>
</tfoot>
</table>
<script type="text/javascript">
<!--
initSort();
//-->
</script>
<@common.bottom/>

