<#import "../../macro/macrocommon.htm" as common>
<@common.header/>
<@common.editheader name="待办事项"/>
<form id="formId" method="post" action="?action=update" >
<input type="hidden" name=id value="${obj.id?default('')}" /><input type="hidden" name=page value="${page?c}"  /><table cellSpacing=1 cellPadding=2 width="96%" align=center bgColor=#cccccc border=0 class="tab">
<thead>
<tr>
<td colspan=4>
编辑待办事项</td>
</tr>
</thead>
<tbody>
<tr>
<td class="titleTd">待办标题</td>
<td colspan="3"><input type="text" class="txtinput" style="width: 96%" name="tname" value="${obj.tname?default('')}" /><span class="spantip" id="tname_span"/></td>
</tr>
<tr>
<td class="titleTd">截至时间</td>
<td><input type="text" class="calendars" name="endTime" value="${obj.endTime}" /><span class="spantip" id="endTime_span"/></td>
<td class="titleTd">待办类型</td>
<td class="contentTd"><input type="text" class="txtinput" name="toDoType" value="${obj.toDoType?default('')}" /><span class="spantip" id="toDoType_span"/></td>
</tr>
<tr>
<td class="titleTd">待办地址</td>
<td colspan="3" bgColor=#ffffff><input type="text" class="txtinput" name="url" value="${obj.url?default('')}" /><span class="spantip" id="url_span"/></td>
</tr>
<tr>
<td colspan=4 height=30 align="center">
<input type="button" class="buttons" value="更新" onclick="save()" />&nbsp;&nbsp;&nbsp;&nbsp;<input type="button" class="buttons" value="关闭" onclick="javascript:history.go(-1);" />
</td>
</tr>
</tbody>
</table>
</form>
<@common.bottom/>

