<#import "../../macro/macrocommon.htm" as common>
<@common.header/>
<@common.editheader name="流程"/>
<form id="formId" method="post" action="?action=update" >
<input type="hidden" name=id value="${obj.id?default('')}" />
<input type="hidden" name=page value="${page?c}"  />
<table cellSpacing=1 cellPadding=2 width="96%" align=center bgColor=#cccccc border=0 class="tab">
<thead>
<tr>
<td colspan=4 bgColor=#f8f8f8 height=30 align="center">
编辑流程</td>
</tr>
</thead>
<tbody>
<tr>
<td class="titleTd">名称</td>
<td class="contentTd"><input type="text" class="txtinput" name="tname" value="${obj.tname?default('')}" /></td>
<td class="titleTd">主键</td>
<td class="contentTd"><input type="text" class="txtinput" name="id" value="${obj.id?default('')}" /></td>
</tr>
<tr>
<td class="titleTd">创建人ID</td>
<td class="contentTd"><input type="text" class="txtinput" name="createId" value="${obj.createId?default('')}" /></td>
<td class="titleTd">创建人姓名</td>
<td class="contentTd"><input type="text" class="txtinput" name="creater" value="${obj.creater?default('')}" /></td>
</tr>
<tr>
<td class="titleTd">创建时间</td>
<td class="contentTd"><input type="text" class="txtinput" name="createTime" value="${obj.createTime}" /></td>
<td class="titleTd">应用名称</td>
<td class="contentTd"><input type="text" class="txtinput" name="appes" value="${obj.appes}" /></td>
</tr>
<tr>
<td class="titleTd">路由参数</td>
<td class="contentTd"><input type="text" class="txtinput" name="routeParameter" value="${obj.routeParameter?default('')}" /></td>
<td class="titleTd">参数类型</td>
<td class="contentTd"><input type="text" class="txtinput" name="parameterType" value="${obj.parameterType?default('')}" /></td>
</tr>
<tr>
<td class="titleTd">版本</td>
<td class="contentTd"><input type="text" class="txtinput" name="version" value="${obj.version?string('')}" /></td>
<td class="titleTd">排序号</td>
<td class="contentTd"><input type="text" class="txtinput" name="sortCode" value="${obj.sortCode?c}" /></td>
</tr>
<tr>
<td class="titleTd">备注描述</td>
<td class="contentTd"><textarea name="description" />${obj.description?default('')} </textarea></td>
<td width=120 bgColor=#f8f8f8></td>
<td class="contentTd"></td>
</tr>
<tr>
<td colspan=4 bgColor=#f8f8f8 height=30 align="center">
<input type="button" class="buttons" value="更新" onclick="save()" />&nbsp;&nbsp;&nbsp;&nbsp;<input type="button" class="buttons" value="关闭" onclick="javascript:history.go(-1);" />
</td>
</tr></tbody>
</table>
</form>
<@common.bottom/>

