<#import "../../macro/macrocommon.htm" as common>
<@common.header/>
<@common.addheader name="图片关联"/>
<form id="formId" method="post" action="?action=save" >
<input type="hidden" id="imagePlayId" name="imagePlayId" value="${imagePlayId?default('')}" />
<table cellSpacing=1 cellPadding=2 width="96%" align=center bgColor=#cccccc border=0 class="tab">
<thead>
<tr>
<td colspan=4 bgColor=#f8f8f8 height=30 align="center">
新建图片关联</td>
</tr>
</thead>
<tbody>
<tr>
<td class="titleTd">图片名称</td>
<td class="contentTd"><input type="text" class="txtinput" name="tname"/></td>
<td class="titleTd">图片地址</td>
<td class="contentTd"><input type="text" class="txtinput" id="imageurl" name="imageurl"/>&nbsp;&nbsp;<a href="#" onclick="openfileuplodpage('image','imageurl')"><font color="red">上传文件</font></a></td>
</tr>
<tr>
<td class="titleTd">连接文本</td>
<td class="contentTd"><input type="text" class="txtinput" name="linktext"/></td>
<td class="titleTd">连接地址</td>
<td class="contentTd"><input type="text" class="txtinput" name="linkurl"/></td>
</tr>
<tr>
<td colspan=4 bgColor=#f8f8f8 height=30 align="center">
<input type="button" class="buttons" value="保存" onclick="save()" />&nbsp;&nbsp;&nbsp;&nbsp;<input type="button" class="buttons" value="关闭" onclick="javascript:history.go(-1);" />
</td>
</tr>
</tbody>
</table>
</form>
<@common.bottom/>

