<#import "../../macro/macrocommon.htm" as common>
<@common.header/>
<@common.editheader name="应用"/>
<form id="formId" method="post" action="?action=update" >
<input type="hidden" name=id value="${obj.id?default('')}" />
<input type="hidden" name=page value="${page?c}"  />
<table cellSpacing=1 cellPadding=2 width="96%" align=center bgColor=#cccccc border=0 class="tab">
<thead>
<tr>
<td colspan=4 bgColor=#f8f8f8 height=30 align="center">
编辑应用信息</td>
</tr>
</thead>
<tbody>
<tr>
<td class="titleTd">名称</td>
<td class="contentTd"><input type="text" class="btinput" required=true style="width: 60%" name="tname" value="${obj.tname?default('')}" /></td>
<td class="titleTd">物理路径</td>
<td class="contentTd"><input type="text" class="txtinput" style="width: 60%" readonly="readonly" name="classPath" value="${obj.classPath?default('')}" /></td>
</tr>
<tr>
<td class="titleTd">类名</td>
<td class="contentTd"><input type="text" class="txtinput" readonly="readonly"  style="width: 60%" name="className" value="${obj.className?default('')}" /></td>
<td class="titleTd">子系统名称</td>
<td class="contentTd"><input type="text" class="txtinput" required=true readonly="readonly" style="width: 60%" name="subSystemName" value="${obj.subSystemName?default('')}" /></td>
</tr>
<tr>
<td class="titleTd">表名</td>
<td class="contentTd" colspan="3"><input type="text" class="btinput" required=true style="width: 60%" name="tableName" value="${obj.tableName?default('')}" /></td>
</tr>
<tr>
<td colspan=4 bgColor=#f8f8f8 height=30 align="center">
<input type="button" class="buttons" value="更新" onclick="save()" />&nbsp;&nbsp;&nbsp;&nbsp;<input type="button" class="buttons" value="关闭" onclick="javascript:history.go(-1);" />
</td>
</tr>
</tbody>
</table>
</form>
<@common.bottom/>

