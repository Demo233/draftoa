<#import "../../macro/mycommon.htm" as common>
<#import "../../macro/paginationCommon.htm" as pagination>
<@common.header/>
<@common.left/>
        <!--sidebar left end-->
        <!--main content start-->
        <section class="main-content-wrapper">
            <section id="main-content">
            
             <div class="row">
                    <div class="col-md-12">
                        <!--breadcrumbs start -->
                        <ul class="breadcrumb">
                            <li><a href="#">主页</a>
                            </li>
                            <li class="active">全部提案</li>
                        </ul>
                        <!--breadcrumbs end -->
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">全部提案</h3>
                                <div class="actions pull-right">
                                    <i class="fa fa-chevron-down"></i>
                                    <i class="fa fa-times"></i>
                                </div>
                            </div>

                            <div class="panel-body">
                                <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%"  style="margin-top:10px;">
                                    <thead>
                                        <tr>
                                            <th>提案标题</th>
                                            <th>提案人</th>
                                            <th>部门</th>
                                            <th>职称</th>
                                            <th>联系电话</th>
                                            <th>附议</th>
                                            <th>赞同票</th>
                                            <th>提交时间</th>
                                            <th>状态</th>                                           
                                        </tr>
                                    </thead>

                                    <tbody>
                                    <#list list as obj>
                                        <tr>
                                            <td>${obj.tname}</td>
                                            <td>${obj.motionMan}</td>
                                            <td>${obj.dept}</td>
                                            <td>${obj.duty}</td>
                                            <td>${obj.mobilNumber}</td>
                                            <td>${obj.fyrNum}</td>
                                            <td>${obj.agreeNum}</td>
                                            <td>${obj.time}</td>
                                            <td><a>签发</a></td>
                                        </tr>
                                      </#list>
                                    </tbody>
                                </table>
                                <table width="100%" border="0" cellpadding="0" cellspacing="0" class="tab">
									<tfoot>
										<tr>
										<td height="22" align="right" bgcolor="#ebeaf4"><@pagination.pagination page=page />&nbsp;</td>
										</tr>
									</tfoot>
								</table>
								<form id="commonform" action="tianQuery.do?action=queryAll" method="post">
      									<input type='hidden' id='page' name='page' value='${page.pageIndex?c}' />
      							</form>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </section>
        <!--main content end-->
<@common.bottom/>
