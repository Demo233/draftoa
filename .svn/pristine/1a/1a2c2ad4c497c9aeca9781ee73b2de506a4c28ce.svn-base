<#import "../../macro/mycommon.htm" as common>
<#import "../../macro/paginationCommon.htm" as pagination>
<@common.header/>
<@common.left/>
        <!--sidebar left end-->
        <!--main content start-->
        <section class="main-content-wrapper">
            <section id="main-content">
             <div class="row">
                    <div class="col-md-12">
                        <!--breadcrumbs start -->
                        <ul class="breadcrumb">
                            <li><a href="#">主页</a>
                            </li>
                            <li class="active">全部提案</li>
                            <li class="active">最新反馈</li>
                        </ul>
                        <!--breadcrumbs end -->
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">最新反馈</h3>
                                <div class="actions pull-right">
                                    <i class="fa fa-chevron-down"></i>
                                    <i class="fa fa-times"></i>
                                </div>
                            </div>

                            <div class="panel-body">
                                <table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
                                    <thead>
                                        <tr>
                                            <th>提案标题</th>
                                            <th>提案人</th>
                                            <th>提交时间</th>
                                            <th>建议处理单位</th>
                                            <th>状态</th>
                                            <th>操作</th>                                           
                                        </tr>
                                    </thead>

                                    <tbody>
                                    <#if list ??>
                                    <#list list as obj>
                                        <tr>
                                            <td>${obj.tname!''}</td>
                                            <td>${obj.createId!''}</td>
                                            <td>${obj.time!''}</td>
                                            <td>${obj.dept!''}</td>
                                            <td><@common.mySwitch str="${obj.status}"/></td>                                            
                                            <td><a href="tianQuery.do?action=latestFeedbackDetail&id=${obj.id}">查看</a></td>
                                        </tr>
									</#list>
									</#if>
                                    </tbody>
                                </table>
								<table width="100%" border="0" cellpadding="0" cellspacing="0" class="tab">
													<tfoot>
														<tr>
														<td height="22" align="right" bgcolor="#ebeaf4"><@pagination.pagination page=page />&nbsp;</td>
														</tr>
													</tfoot>
												 </table>
												<form id="commonform" action="tianQuery.do?action=latestSubmit" method="post">
				      									<input type='hidden' id='page' name='page' value='${page.pageIndex?c}' />
				      							</form>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </section>
        <!--main content end-->
<@common.bottom/>
